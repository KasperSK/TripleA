<Window x:Class="CashRegister.GUI.Dialogs.PaymentDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CashRegister.GUI.Dialogs"
        mc:Ignorable="d"
        Title="PaymentDialog" Height="150" Width="600">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Button Name="Cash" Grid.Column="0" Content="Cash" IsDefault="True" Style="{StaticResource NormalButton}"/>
        <Button Name="Dankort" Grid.Column="1" Content="Dankort" Style="{StaticResource NormalButton}"/>
        <Button Name="MobilePay" Grid.Column="2" Content="MobilePay" Style="{StaticResource NormalButton}"/>
    </Grid>
</Window>
